    "def maxSubArraySum(arr,N):\n",
    "    maxsum = arr[0]\n",
    "    sum = 0\n",
    "    for i in range(N) :\n",
    "        sum = sum +arr[i]\n",
    "        maxsum = max(maxsum, sum)\n",
    "        if sum<0:\n",
    "            sum =0\n",
    "    return maxsum\n",
    "print(maxSubArraySum([1,2,3,-2,5], 5))"
   ]
  }
